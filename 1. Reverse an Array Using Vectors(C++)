#include<bits/stdc++.h>
using namespace std;
void rev(vector<int> &v) {
    reverse(v.begin(),v.end());
}

int main() {
    //Vector Size
    int n; cin>>n;
    vector<int> v;
    int ele;
    //Vector Elements
    for(int i=0;i<n;i++) {
        cin>>ele; v.push_back(ele);
    }
    rev(v);
    for(int i=0;i<n;i++) {
        cout<<v[i]<<" ";
    }
}
